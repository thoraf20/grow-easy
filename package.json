{
  "name": "grow-easy",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "db:seed": "ts-node src/scripts/seed.ts",
    "db:migrate": "ts-node src/scripts/migrate.ts",
    "db:reset": "ts-node src/scripts/reset.ts",
    "start": "node build/src/server.js",
    "dev": "nodemon index.js",
    "build": "tsc -p . && cp package.json ./build/package.json && cp package-lock.json ./build/package-lock.json && cp .env ./build/.env",
    "lint": "eslint src/**/*.ts",
    "lint-fix": "eslint --fix src/**/*.ts",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "prepare": "husky",
    "test:custom": "npm test --  tests/unit-tests/lib/crypto.spec.ts",
    "precommit": "npm run lint-fix && npm run format",
    "prepush": "npm run lint ",
    "test": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "author": "Toheeb Rauf",
  "license": "Apache-2.0",
  "dependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.14.6",
    "bcrypt": "^6.0.0",
    "cloudinary": "^2.7.0",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.19.2",
    "express-rate-limit": "^7.5.0",
    "helmet": "^7.1.0",
    "http-status-codes": "^2.3.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.15.2",
    "morgan": "^1.10.0",
    "swagger-ui-express": "^5.0.1",
    "typescript": "^5.4.5",
    "uuid": "^11.1.0",
    "winston": "^3.13.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.5.0",
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.17",
    "@types/dotenv": "^6.1.1",
    "@types/eslint__js": "^8.42.3",
    "@types/express-rate-limit": "^5.1.3",
    "@types/jest": "^29.5.12",
    "@types/joi": "^17.2.2",
    "@types/jsonwebtoken": "^9.0.10",
